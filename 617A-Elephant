#include<stdio.h>
int main()
{
    int n;

    scanf("%d",&n);

    int i=0,count=0;
    while(i!=n)
    {
        if(i+5<=n)
        {
            count++;
            i+=5;
        }
        else if(i+4<=n)
        {
            count++;
            i+=4;
        }
        else if(i+3<=n)
        {
            count++;
            i+=3;
        }
        else if(i+2<=n)
        {
            count++;
            i+=2;
        }
        else if(i+1<=n)
        {
            count++;
            i+=1;
        }
    }
    printf("%d\n",count);
    return 0;
}
