#include<bits/stdc++.h>
using namespace std;
#define Royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define max1 10000
int main()
{
    Royscode();
string s="hello";
string s1;
cin >> s1;
int flag=0,j=0;
for(int i=0;i<s1.size();i++)
{
    if(s1[i]==s[j])
    {
        flag++;
        j++;
        if(flag==5)
        {
            break;
        }

    }
}

if(flag==5)
{
    cout << "YES" << endl;
}
else{
    cout << "NO" << endl;
}
    return 0;
}
