#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define max1 10000
int main()
{
    string s;
    cin>>s;
    vector<char>v;
    for(ll i=0;i<s.size();i++)
    {
        if(s[i]=='W' && s[i+1]=='U' && s[i+2]=='B')
        {
            v.push_back(' ');
            i+=2;
        }
        else
        {
            v.push_back(s[i]);
        }

    }
    for(auto u:v)cout<<u;
    return 0;
}
