#include<bits/stdc++.h>
using namespace std;
#define Royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define max1 10000
int main()
{
    Royscode();
string s;
cin >> s;
//int j;
//vector<int>v;
int flag=1;
int m=1;
for(int i=0;i<s.size();i++)
{
    if(s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U' || s[i]=='Y')
    {
        flag=1;
    }
    else{
        flag++;
        m=max(m,flag);
    }
}
cout << m << endl;
    return 0;
}
