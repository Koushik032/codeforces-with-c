#include<bits/stdc++.h>
using namespace std;
#define Royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define max 10000
#define ll long long
int main()
{
    int n;
    cin >> n;
    string s;
    for(int i=0;i<n;i++)
    {
        cin >> s;
        int a=stoi(s);
        int sum=0;
        int sum1=0;
        for(int i=0;i<3;i++)
        {
            sum+=s[i];
        }
        for(int i=3;i<6;i++)
        {
            sum1+=s[i];
        }

        if(sum==sum1)
        {
            cout << "YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
    }
}
