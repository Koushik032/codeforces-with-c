#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
int main()
{
    optimize();
int t;
cin >> t;
while(t--){
int arr[10000];
  for(int i=0;i<7;i++)
  {
    cin >> arr[i];
  }

    int c=arr[7-1]-abs(arr[0]+arr[1]);

    cout << arr[0] << " " << arr[1] << " " << c << endl;
}
    return 0;
}
