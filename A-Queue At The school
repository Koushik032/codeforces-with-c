#include<bits/stdc++.h>
using namespace std;
#define royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';

int main()
{

    royscode();
    int n,i,l,t,j;
    cin >> n >> t;
    string s;
    cin >> s;
    for(i=0; i<t; i++)
    {
        for(j=0; s[j]!='\0'; j++)
        {
            if(s[j]=='B'&& s[j+1]=='G')
            {
                s[j]='G';
                s[j+1]='B';
                j++;
            }
        }
    }
     cout << s << endl;
    return 0;
}
