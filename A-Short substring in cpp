#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
int main()
{
    int t;
    cin >> t;

    while(t--)
    {
      string s;
      cin >> s;

      int len=s.size();

      for(int i=0;i<s.size()-1;i=i+2)
      {
        cout << s[i];
      }
      cout << s[s.size()-1];

      cout << endl;
    }
  return 0;
}
