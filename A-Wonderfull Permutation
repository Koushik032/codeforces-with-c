#include<bits/stdc++.h>
using namespace std;
#define Royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define max1 10000
int main()
{
    Royscode();
   int t;
   cin >> t;

   while(t--)
   {
    int n,k;
    cin >> n >> k;

    int a[n];
    for(int i=0;i<n;i++)
    {
        cin >> a[i];
    }

    int flag=0;
    for(int i=0;i<k;i++)
    {
        if(a[i]>k)
        {
            flag++;
        }
    }

    cout << flag << endl;
   }
    return 0;
}
