#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define max1 10000
int main(){
   ll t;
   cin>>t;
   ll sum=0;
   while(t--)
   {
    ll n;
    cin>>n;

        for(ll i=2;i<30;i++)
        {
            sum=(1<<i)-1;
            if(n%sum==0)
            {
                cout<<n/sum<<endl;
                break;
            }
        }  
   }
    return 0;
}
   
