#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define max1 10000
#define pb push_back
#define fi first
#define se second
#define vst() sort(v.begin(),v.end())
int main()
{
    ll n;
    cin>>n;
    vector<ll>v;
    vector<ll>v1;

    if(n%2!=0){
        cout<<-1<<endl;
    }
    else{
        for(ll i=1;i<=n;i++)
        {
            if(i%2!=0){
                v.pb(i+1);
            }
            else{
                v.pb(i-1);
            }
        }
    }
    for(auto u:v)cout<<u<<" ";

    return 0;
}

