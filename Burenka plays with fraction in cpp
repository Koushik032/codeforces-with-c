#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define ll long long
int main()
{
    ll n;
    cin >> n;

   while(n--)
   {
    ll a,b,c,d;
    cin >> a >> b >> c >> d;

    ll x=a*d;
    ll y=b*c;

    if(x==y){
        cout << "0" << endl;
    }
    else if(x!=0 && y%x==0 || y!=0 && x%y==0)
    {
        cout << "1" << endl;
    }
    else{
        cout << "2" << endl;
    }

   }
    return 0;
}
