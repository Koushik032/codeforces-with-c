#include<bits/stdc++.h>
using namespace std;
#define Royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define max1 10000
#define ll long long
int main()
{
    Royscode();
    int k,r;
    cin >> k >> r;
    int i=1;
    while(1)
    {
        int sum=k*i;
        int rem=sum%10;
        if((rem==r) || (rem==0))
        {
            break;
        }
        else{
            i++;
        }
    }
    cout << i << endl;
    return 0;
    }
