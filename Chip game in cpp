#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define ll long long
int main()
{
    ll n;
    cin >> n;

    while(n--)
    {
        ll a,b;
        cin >> a >> b;

        ll x=a-b;

        if(x%2==0)
        {
            cout << "Tonya" << endl;
        }
        else{
            cout << "Burenka" << endl;
        }

    }
    return 0;
}
