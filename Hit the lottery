#include<stdio.h>
#define max 10000
int main()
{
    int n,count=0;

    scanf("%d",&n);

    while(n!=0)
    {
        if(n>=100)
        {
            count++;
            n=n-100;
        }
        else if(n>=20)
        {
            count++;
            n=n-20;
        }
        else if(n>=10)
        {
            count++;
            n=n-10;
        }
        else if(n>=5)
        {
            count++;
            n=n-5;
        }
        else if(n>=1)
        {
            count++;
            n=n-1;
        }
    }
    printf("%d",count);

    return 0;
}
