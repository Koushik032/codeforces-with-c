#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef vector<ll> vl;
typedef pair<int,int> pii;

#define endl '\n'
#define PB push_back
#define F first
#define S second
#define all(a) (a).begin(),(a).end()
#define rall(a) (a).rbegin(),(a).rend()

#define MOD 1000000007

#define mem(a,b) memset(a, b, sizeof(a) )
#define sqr(a) ((a) * (a))

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);


ll gcd ( ll a, ll b )
{ 
    return __gcd ( a, b );
}
ll lcm ( ll a, ll b )
{
    return a * ( b / gcd ( a, b ) );
}

//const int mx =1e5+123;
//bitset <mx> isprime;
//vector <int> primes;


int main()
{
    optimize();

        vi v(5);
        int sum = 0;

        for ( auto &i : v )
            {
                cin >> i;
                sum += i;
            }

            if ( sum % 5 == 0 && sum != 0 )
                cout << sum / 5 << endl;
            else cout << -1 << endl;

    return 0;
}
