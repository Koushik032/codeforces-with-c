#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define max1 10000
#define pb push_back
#define fi first
#define se second
#define vst() sort(v.begin(),v.end())
#define vec() vector<ll>v
int main()
{
    ll t;
    cin>>t;
    while(t--){
        vec();
        ll a,b,c;
        cin>>a;
        v.pb(a);
        cin>>b;
        v.pb(b);
        cin>>c;
        v.pb(c);
        vst();

        cout<<v[1]<<endl;

        v.clear();
    }
    return 0;
}
