#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef vector<ll> vl;
typedef pair<int,int> pii;

#define endl '\n'
#define PB push_back
#define F first
#define S second
#define all(a) (a).begin(),(a).end()
#define rall(a) (a).rbegin(),(a).rend()

#define MOD 1000000007

#define mem(a,b) memset(a, b, sizeof(a) )
#define sqr(a) ((a) * (a))

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);


ll gcd ( ll a, ll b )
{ 
    return __gcd ( a, b );
}
ll lcm ( ll a, ll b )
{
    return a * ( b / gcd ( a, b ) );
}
const int mx =1e5+123;
// int arr[mx], arr1[mx];
//bitset <mx> isprime;
//vector <int> primes;
int arr[mx];

int main()
{
    optimize();
        
        ll t;
        cin >> t;

        while ( t-- )
        {
            int n, a, b , c , d;
            cin >> n >> a >> b >> c >> d;

            int wei = n * (a-b);
            int wei1 = n * (a+b);

            if ( wei1 < c-d || wei > c + d )
            {
                cout << "NO" << endl;
            }
            else cout << "YES" << endl;
        }

    return 0;
}
