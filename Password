#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin >>t;
    while(t--){
        int n,x=0;
        cin>> n;
        int a[n+5];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        x=abs(10-n);
        //cout<<x<<endl;
        if(x==2)
        {
            cout<<"6"<<endl;
        }
        else if(x==3){
            cout<<"18"<<endl;
        }
        else if(x==4){
            cout<<"36"<<endl;
        }
        else if(x==5){
            cout<<"60"<<endl;
        }
        else if(x==6){
            cout<<"90"<<endl;
        }
        else if(x==7){
            cout<<"126"<<endl;
        }
        else if(x==8){
            cout<<"168"<<endl;
        }
        else{
            cout<<"216"<<endl;
        }
    }
    return 0;
}
