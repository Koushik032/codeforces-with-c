#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin >>t;
    while(t--){
        int n;
        cin>>n;
        vector<int>v;
        for(int i=1;i<=n;i++)
        {
            if(i==1)
            {
                v.push_back(i);
            }
            else if(i>1 && i<n){
                v.push_back(i+1);
            }
            else if(i==n)
            {
                v.push_back(2);
            }
        }
        for(auto u:v)cout<<u<<" ";
            cout<<endl;
    }
    return 0;
}
