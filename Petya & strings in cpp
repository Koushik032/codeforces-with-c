#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
int main(){

string s,s1;

cin >> s;
cin >> s1;


for(long long i=0;i<s[i]!='\0';i++)
{
    s[i]=tolower(s[i]);
}
for(long long i=0;i<s1[i]!='\0';i++)
{
    s1[i]=tolower(s1[i]);
}
if(s<s1)
{
    cout << "-1" << endl;
}
else if(s1<s)
{
    cout << "1" << endl;
}
else{
    cout << "0" <<endl;
}
    return 0;
}
