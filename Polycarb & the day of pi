#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
    ll t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        ll cnt=0;
        string s1="3141592653589793238462643383279";

        for(ll i=0;i<s.size();i++){
            if(s[i]==s1[i])cnt++;
            else break;
        }
        cout<<cnt<<endl;
    }
return 0;
}
