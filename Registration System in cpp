#include<bits/stdc++.h>
using namespace std;
#define royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define ll long long
int main()
{
    royscode();
    int t;
    cin >> t;
    map< string, int > cnt;
    while(t--)
    {
        string s;
        cin >> s;
        //cnt[s]++;

        if(cnt[s]==0)
        {
            cout << "OK" << endl;
        }
        else{
            cout << s << cnt[s] << endl;
        }
        cnt[s]++;
    }
    return 0;
}
