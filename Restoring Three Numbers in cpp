#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
int main()
{
    int arr[4];

    for(int i=0;i<4;i++)
    {
        cin >> arr[i];
    }

    sort(arr,arr+4);

    int c=arr[3]-arr[0];

    cout << arr[2]-c << " " << arr[1]-c << " " << c;
    return 0;
}
