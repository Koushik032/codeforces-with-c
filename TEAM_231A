#include<stdio.h>
int main()
{
    int n,i,j,m,count=0,sum=0;

    scanf("%d",&n);

    for(i=0; i<n; i++)
    {
    count=0;
        for(j=0; j<3; j++)
        {
            scanf("%d",&m);

            if(m==1)
            {
                count++;
            }
        }
        if(count>=2)
        {
            sum++;
        }
        count=0;
    }
    printf("%d\n",sum);

    return 0;
}
