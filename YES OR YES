#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define max1 10000
#define pb push_back
#define fi first
#define se second
#define vst() sort(v.begin(),v.end())
#define vec() vector<string>v
int main()
{
    ll t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;

        string s1="YesYesYesYesYesYesYesYesYesYesYesYesYesYesYesYesYesYesYesYes";

        if(s1.find(s)==-1){
            cout<<"NO"<<endl;
        }
        else{
            cout<<"YES"<<endl;
        }
    }
    return 0;
}
