#include<stdio.h>
int main()
{
    int n,a[10000],b[10000],c[10000],i,sum_a=0,sum_b=0,sum_c=0;

    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
        scanf("%d%d%d",&a[i],&b[i],&c[i]);

    }
    for(i=0;i<n;i++)
    {
        sum_a=sum_a+a[i];
        sum_b=sum_b+b[i];
        sum_c=sum_c+c[i];
    }
    if(sum_a==0 && sum_b==0 && sum_c==0)
    {
        printf("YES\n");
    }
    else
    {
        printf("NO\n");
    }

    return 0;
}
