#include<stdio.h>
int main()
{
    int n,sum=0,i;
    char a[1000];

    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
        scanf("%s",a);

        if(a[0]=='T')
        {
            sum+=4;
        }
        else if(a[0]=='C')
        {
            sum+=6;
        }
        else if(a[0]=='O')
        {
            sum+=8;
        }
        else if(a[0]=='D')
        {
            sum+=12;
        }
        else if(a[0]=='I')
        {
            sum+=20;
        }
    }
    printf("%d\n",sum);
}
